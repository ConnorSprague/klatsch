package edu.iu.cnets.klatsch.exception;


/**
 * This exception is thrown by the Registry class for all errors that occur when
 * accessing the already-initialized registry.
 */
public class RegistryException extends KlatschException
{
	/** make Eclipse happy */
	private static final long serialVersionUID = 3177321945121882991L;
	
	/** the error message */
	String message;
	

	/**
	 * Initializes with an error message.
	 * 
	 * @param message  the error message
	 */
	public RegistryException(String message)
	{
		this.message = message;
	}
}
