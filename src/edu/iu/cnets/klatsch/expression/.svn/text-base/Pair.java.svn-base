package edu.iu.cnets.klatsch.expression;


/**
 * A bind is a basic wrapper for an entry in a dictionary; a key expression and a value expression.
 */
public class Pair
{
	public final Expression expKey, expVal;
	
	
	public Pair(Expression expKey, Expression expVal)
	{
		this.expKey = expKey;
		this.expVal = expVal;
	}
	
	
	public String toString()
	{
		return expKey.toString() + ":" + expVal.toString(); 
	}
}
